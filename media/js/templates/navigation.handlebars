<div style="text-align: center;">
	<div id="title">
		<label class="bigLabel" style="color:#DDD;">Scraping Hub Autoscraping v0.3</label>
		<img style="margin-left:5px;" src="minispider.png"/>
	</div>
	<div id="navigationbar" class="ui-corner-all">
		{{#each routeStack}}
			<div style="margin:4px 0px 4px 0px;">
				{{view ASTool.ButtonView action="gotoRoute" selected='true' argument=this.route label=this.label minWidth='350px' icon="ui-icon-circle-arrow-s"}}
			</div>
		{{/each}}
	</div>
</div>
