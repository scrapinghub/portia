<div style="margin:10px 0px 0px 10px">
	<div style="margin-top:10px"></div>
	<h4>Open project</h4>
	<div class="scrolling-container ui-corner-all">
		{{#each}}
			<div style="margin:4px 0px 4px 0px">
				{{view ASTool.ButtonView action="openProject" argument=this label=this minWidth='85%' class="clear-button"}}
				{{view ASTool.ButtonView action="deleteProject" argument=this icon="ui-icon-trash" class="textless-button red-button"}}
			</div>
		{{else}}
			<h5>No projects have been created yet.</h5>
		{{/each}}
	</div>
	
</div>

